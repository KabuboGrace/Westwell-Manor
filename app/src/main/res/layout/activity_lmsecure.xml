<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#9A6AF1"
    tools:context=".LMsecure">

    <include
        android:id="@+id/include3"
        layout="@layout/toolbar3" />


    <Button
        android:id="@+id/btnfire"
        android:layout_width="236dp"
        android:layout_height="62dp"
        android:backgroundTint="@color/white"
        android:text="Fire"
        android:textColor="#9A6AF1"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.222" />

    <Button
        android:id="@+id/btnRobbery"
        android:layout_width="236dp"
        android:layout_height="71dp"
        android:backgroundTint="@color/white"
        android:text="Crime"
        android:textColor="#9A6AF1"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.398" />

    <Button
        android:id="@+id/btnanimal"
        android:layout_width="245dp"
        android:layout_height="81dp"
        android:backgroundTint="@color/white"
        android:text="Animal Invasion"
        android:textColor="#9A6AF1"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.794" />

    <Button
        android:id="@+id/btnMedical"
        android:layout_width="248dp"
        android:layout_height="73dp"
        android:backgroundTint="@color/white"
        android:text="Medical"
        android:textColor="#9A6AF1"
        android:textSize="20dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.588" />


</androidx.constraintlayout.widget.ConstraintLayout>